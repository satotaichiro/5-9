<h2>Users</h2>

	<table>
	<tr>
		<th>name</th>
	</tr>

	<!--取得・一覧のリストをここに出す-->
	<% @users.each do |user| %>
	<tbody>
	<tr>
		<td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: "jpg", fallback: "no-image-icon.jpg" %></td>
		<td><%= user.name %></td>
		<td><%= link_to "show", user_path(user.id) %>
		</td>
	</tr>
	</tbody>
	<% end %>
</table>
